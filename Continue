//We can use continue to to skip the iteration.
//continue in a 'for' loop

#include <iostream>
using namespace std;

int main (){
	int a;
	
	for (int i=1; i<5; i++) {
		for (int j=1; j<7; j++) {
		
			if(j==6) {
				continue;     //when j is 6, it skips the iteration.
			}
			cout<<"i is "<<i<<","<<"j is"<<j<<"\n";
		}
	}
	
	
	return 0;
	
}
/*
when i = 1
  when j = 1 (print)
  when j = 2 (print)
  when j = 3 (print)
  when j = 4 (print)
  when j = 5 (print)
  when j = 6 (skip)
when i = 2
  when j = 1 (print)
  when j = 2 (print)
  when j = 3 (print)
  when j = 4 (print)
  when j = 5 (print)
  when j = 6 (skip)
when i = 3
  when j = 1 (print)
  when j = 2 (print)
  when j = 3 (print)
  when j = 4 (print)
  when j = 5 (print)
  when j = 6 (skip)
when i = 4
  when j = 1 (print)
  when j = 2 (print)
  when j = 3 (print)
  when j = 4 (print)
  when j = 5 (print)
  when j = 6 (skip)  
*/

---example
